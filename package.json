{
  "name": "react-recruitment-template",
  "private": true,
  "author": "Mindgram",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "initHusky": "husky install",
    "changeHuskyToExecutable": "chmod +x .husky/*",
    "prepare": "npm run initHusky && npm run changeHuskyToExecutable",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --incremental",
    "prettify": "prettier --check .",
    "prettify:fix": "prettier --check --write .",
    "lint": "eslint src --ext ts,tsx --max-warnings 0 --cache --report-unused-disable-directives",
    "lint:fix": "eslint src --ext ts,tsx --max-warnings 0 --fix --cache --report-unused-disable-directives"
  },
  "lint-staged": {
    "src": [
      "npm run lint:fix",
      "npm run prettify:fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@react-hookz/web": "^23.1.0",
    "@tanstack/react-query": "^4.29.19",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^13.0.2",
    "react-router-dom": "^6.14.1",
    "styled-components": "^5.3.11",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@swc/plugin-styled-components": "^1.5.70",
    "@tanstack/react-query-devtools": "^4.29.19",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash-es": "^4.17.8",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.33.0",
    "whatwg-fetch": "^3.6.16"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
